# Read the Verilog file
read_verilog input_verilog_file.v

# Identify the module
hierarchy -top counter

# Extract the input and output ports from the D flip-flops
extract

# Flatten the design (remove hierarchy)
flatten

# Remove D flip-flops
opt -fine -mux_undef -undriven -full

# Write the modified Verilog code to a new file
write_verilog output_verilog_file.v
